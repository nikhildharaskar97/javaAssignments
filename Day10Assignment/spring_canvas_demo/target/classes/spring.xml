       <bean id="line1" class="com.telstra1.spring_canvas_demo.Line">
       		<property name="p1" ref="pl1"/>
       		<property name="p2" ref="pl2"/>
        </bean>
        
        <bean id="triangle1" class="com.telstra1.spring_canvas_demo.Triangle">
